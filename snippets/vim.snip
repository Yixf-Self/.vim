snippet     suite
abbr        themis suite
options     head
	let ${1:s:suite} = themis#suite('${2}')${0}

snippet     assert
abbr        themis assert
options     head
	let s:assert = themis#helper('${1:assert}')${0}

snippet     fun_themis
abbr        themis test function
options     head
	function! s:suite.${1:#:test_case}()
		${0}
	endfunction

